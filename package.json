{
  "name": "valorant-tips-frontend",
  "homepage": "http://gabrielogregorio.github.io/valorant-tips",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "jest": "jest --passWithNoTests",
    "test": "jest && yarn run cypress:headless",
    "jest:coverage": "jest --coverage",
    "jest:watchAll": "jest --watchAll",
    "jest:watchAll:coverage": "jest --watchAll --coverage",
    "cypress": "cross-env NODE_ENV=test cypress open",
    "cypress:headless": "cross-env NODE_ENV=test cypress run --headless --browser chrome",
    "deprecated:e2e:headless": "next build && start-server-and-test start http://localhost:3000 cypress:headless",
    "type-check": "tsc",
    "lint": "next lint",
    "prepare": "husky install",
    "stylelint": "stylelint '**.css'",
    "check-all": "tsc && next lint && stylelint '**.css' && jest --coverage && jest-coverage-badges && yarn cypress:headless && yarn audit --groups 'dependencies'",
    "storybook": "start-storybook -p 6006 -s ./public",
    "storybook:build": "build-storybook -s public",
    "predeploy:deprecated": "yarn run build-storybook",
    "deploy-storybook:deprecated": "gh-pages -d storybook-static",
    "deploy-storybook-gh-pages:deprecated": "yarn run build-storybook && yarn run deploy-storybook"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@sentry/nextjs": "^7.102.1",
    "axios": "^1.6.7",
    "next": "14.1.0",
    "next-pwa": "5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-icons": "^5.0.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-query": "^3.39.3",
    "sharp": "^0.33.2",
    "uuid": "^9.0.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/code-coverage": "^3.12.26",
    "@react-mock/localstorage": "^0.1.2",
    "@storybook/addon-actions": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^7.6.17",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^7.6.17",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/chai": "^4.3.12",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.59",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "cross-fetch": "^4.0.0",
    "cypress": "^13.6.6",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-cypress": "^0.28.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-deprecate": "^0.8.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "express": "^4.18.2",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "msw": "0.39.2",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "whatwg-fetch": "^3.6.20"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "cross-env CI=true yarn jest"
    ]
  }
}
